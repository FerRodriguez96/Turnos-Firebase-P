{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/login.html",
        "destination": "/login.html"
      },
      {
        "source": "/**",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  },
  "database": {
    "rules": "database.rules.json"
  }
}
